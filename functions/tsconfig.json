    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2015",
    "suppressImplicitAnyIndexErrors":true,
    "noImplicitAny":false
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
